<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>

  <connectionStrings>
    <add name="ConnectionString" connectionString="Data Source=(local);Initial Catalog=Sports Manager;Integrated Security=SSPI;"/>
  </connectionStrings>
  
    <add dataSource="(local)"/>
    <add key="dbName" value="Sports Manager"/>
    <add key="authentification" value="Integrated Security=SSPI"/>
    <add key="parentTableName" value="Teams"/>
    <add key="childTableName" value="Locations"/>
    <add key="primaryKey" value="Location_ID"/>
    <add key="foreignKey" value="Home_Team_ID"/>
  

</configuration>